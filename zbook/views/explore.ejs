<!DOCTYPE html>
<html lang="en">

<head>
    <%- include('./partials/headlinks.ejs') %>
</head>

<body>
    <%- include('./partials/header') %>
    <main class="explore">
        <% listaPosts.forEach(element => { %>
                        <% idusuarioSearched = element.idusuario %>
                        <% nombrePoster = element.user.username %>
                        <% idPoster = element.idusuario %>
                        <% profPic = element.user.profilePicture %>
        <div class="post-explore">
            <a href="/post/<%= element.id %>"><img src="<%= element.url %>" alt=""></a>
        </div>             
        <% }); %>
    </main>
    <%- include('./partials/footer') %>
</body>
</html>